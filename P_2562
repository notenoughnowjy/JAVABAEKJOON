import java.util.*;
public class P_2562 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = 9;
		int max = 0;
		int index = 1;
		int arr[] = new int[N];
		for(int i=0;i<N;i++) {
			arr[i] = sc.nextInt();
			
			if(arr[i]>max) {
				max = arr[i];
				index = i+1;
			}
		}
		System.out.println(max);
		System.out.println(index);
	}

}
